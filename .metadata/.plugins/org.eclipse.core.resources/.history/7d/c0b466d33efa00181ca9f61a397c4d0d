package ie.gmit.models;

public class Account {
	
	private String Name;
	private int Age;
	
	public Account(String name, int age) {
		super();
		
		Name = name;
		Age = age;
	}
	
	public String getName() {
		return Name;	
	}

	public int getAge() {
		return Age;
	}

	public void setAge(int age) {
		Age = age;
	}

	public void setName(String name) {
		Name = name;
	}
	
	

}
